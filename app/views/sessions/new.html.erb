<h2>Sign in to an existing user</h2>

<%= form_with url: sign_in_path, method: :post, html: {class: "form-vertical signin-form"} do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Sign In"%>
<% end %>
